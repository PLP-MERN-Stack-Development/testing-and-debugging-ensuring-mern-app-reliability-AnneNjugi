{
  "name": "client",
  "version": "1.0.0",
  "description": "React client with testing",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --config ../jest.config.js --selectProjects client",
    "test:unit": "jest --config ../jest.config.js --selectProjects client --testPathPattern='unit'",
    "test:integration": "jest --config ../jest.config.js --selectProjects client --testPathPattern='integration'",
    "test:watch": "jest --config ../jest.config.js --selectProjects client --watch",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.0.8",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "babel-jest": "^29.7.0",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "identity-obj-proxy": "^3.0.0",
    "cypress": "^13.6.2"
  }
}
